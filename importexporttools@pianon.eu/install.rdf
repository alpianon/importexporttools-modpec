<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
xmlns:em="http://www.mozilla.org/2004/em-rdf#">

	<Description about="urn:mozilla:install-manifest">
	
		<em:id>importexporttools@pianon.eu</em:id>
		<em:name>ImportExportTools_modPEC</em:name>
		<em:version>3.2.4.1-modPEC</em:version>
		<em:description>Adds tools to export/import messages and folders (with PEC backup option)</em:description>
		<em:creator>Paolo Kaosmos (mod by Alberto Pianon)</em:creator>
		<em:optionsURL>chrome://mboximport/content/mboximportOptions.xul</em:optionsURL>
		<em:homepageURL>https://github.com/alpianon/importexporttools-modpec</em:homepageURL>
		<em:file>
			<Description about="urn:mozilla:extension:file:mboximport.jar">
				<em:package>content/mboximport/</em:package>
				<em:locale>locale/en-US/mboximport/</em:locale>
				<em:locale>locale/it/mboximport/</em:locale>
			</Description>
		</em:file>

		<em:targetApplication>
			 <!-- Thunderbird -->
			<Description>
				 <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
        				<em:minVersion>1.0</em:minVersion>
        				<em:maxVersion>35.*</em:maxVersion>
			</Description>
		</em:targetApplication>

		<em:targetApplication>
		      <!-- Postbox -->
			<Description>
				<em:id>postbox@postbox-inc.com</em:id>
				<em:minVersion>1.0b13</em:minVersion>
				<em:maxVersion>3.*</em:maxVersion>
			</Description>
		</em:targetApplication>

		  <!-- SeaMonkey -->
		 <em:targetApplication>
			<Description>
				<em:id>{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}</em:id>
				<em:minVersion>2.0</em:minVersion>
				<em:maxVersion>2.5</em:maxVersion>
			</Description>
		</em:targetApplication>

	</Description>

</RDF>
